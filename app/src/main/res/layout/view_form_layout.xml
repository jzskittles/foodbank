            android:maxLines="1"
            android:text="LastDefault"
            android:textColor="#000"
            android:paddingLeft="5dp"
            android:paddingTop="3dp"
            android:paddingRight="16dp"
            android:paddingBottom="10dp"/>
    </LinearLayout>

    <LinearLayout android:layout_height = "wrap_content"
